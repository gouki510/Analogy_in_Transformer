# Emergent Analogy - Environment Variables
# Copy this file to .env and edit as needed
# Usage: cp env.example .env

# ===================
# WandB Configuration
# ===================

# Your WandB API Key (get from https://wandb.ai/authorize)
WANDB_API_KEY=

# WandB Project name
WANDB_PROJECT=emergent_analogy

# WandB Run name (optional, auto-generated if empty)
WANDB_RUN_NAME=

# ===================
# GPU Configuration
# ===================

# Comma-separated GPU IDs to use (e.g., "0" or "0,1")
# Set to -1 to use CPU only
CUDA_VISIBLE_DEVICES=0
